
// User defined label
public class DominoLabel {
	String top = "";
	String bottom = "";
	
	public DominoLabel() {}
	
	public DominoLabel(String _top, String _bottom) {
		top = _top;
		bottom = _bottom;
	}

	public String getTop() {
		return top;
	}

	public void setTop(String top) {
		this.top = top;
	}

	public String getBottom() {
		return bottom;
	}

	public void setBottom(String bottom) {
		this.bottom = bottom;
	}
	
	public String toString() {
		return "[" + top + "/" + bottom + "]";
	}
}
